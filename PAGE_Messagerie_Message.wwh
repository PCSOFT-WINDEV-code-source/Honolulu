   -
     name : InitialiseActions
     procedure_id : 1557706104959674266
     type_code : 14
     code : |1+
      Procédure InitialiseActions(sCombo)
      
      
      	ListeSupprimeTout(sCombo)
      	
      	// Actions simples
      	ListeAjoute(sCombo, gLien(-1) + <§$0001§>)
      	ListeAjoute(sCombo, gLien(-2) + Répète(ESP, 5) + <§$0002§>)
      	ListeAjoute(sCombo, gLien(-3) + Répète(ESP, 5) + <§$0003§>)
      	
      	// Appliquer les étiquettes ?
      	REQ_Messagerie_Etiquettes.pIDUtilisateur = UtilisateurEnCours:IDUtilisateur
      	HExécuteRequête(REQ_Messagerie_Etiquettes)
      	
      	SI HNbEnr(REQ_Messagerie_Etiquettes) > 0 ALORS
      		
      		ListeAjoute(sCombo, gLien(-4) + <§$0004§>)	
      		ListeAjoute(sCombo, gLien(-1) + <§$0005§>)
      		
      		// Ajoute les étiquettes
      		POUR TOUTE REQ_Messagerie_Etiquettes
      			ListeAjoute(sCombo, gLien(REQ_Messagerie_Etiquettes.IDEtiquette) + Répète(ESP, 5) + REQ_Messagerie_Etiquettes.Libellé)
      		FIN
      	FIN
      	
      	sCombo..Etat = Grisé
      
      
