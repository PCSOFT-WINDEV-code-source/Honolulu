   -
     name : ChargeMessage
     internal_properties : 
     procedure_id : 1557715579661897696
     type_code : 14
     code : |1-
      // Résumé : Charge le message en cours
      // Syntaxe :
      // ChargeMessage ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure ChargeMessage()
      
      	sExpéditeur			est une chaine
      	sListeDestinataires est une chaine
      	sListePiècesJointes est une chaine
      	sSignature			est une chaine
      	sHTMPiècesJointes 	est une chaine
      	
      
      	// Récupère les destinataires
      	pour tout Destinataire avec IDMessage = gnIDMessage
      		
      		// On n'affiche pas les destinataires cachés
      		si Destinataire.EstDestinataireCaché alors continuer
      			
      		// On ne tient pas compte de l'utilisateur courant
      		si Destinataire.IDUtilisateur = UtilisateurEnCours:IDUtilisateur ALORS 
      			// Premier destinataire : utilisateur en cours
      			sListeDestinataires += [", "] + <§$0022§>
      			continuer
      		FIN
      				
      		// Récupère l'utilisateur
      		si Destinataire.IDUtilisateur <> -1 alors
      			// C'est un utilisateur connu, on affiche son nom
      			sListeDestinataires += [", "] + RécupèreNomUtilisateur(Destinataire.IDUtilisateur)
      		sinon
      			// C'est un utilisateur externe, on affiche son email
      			sListeDestinataires += [", "] + RécupèreNomAffichéEmailEx(Destinataire.EmailDestinataire)
      		FIN
      		
      	fin
      	
      	// Par défaut, le message est nécessairement envoyé à l'utilisateur
      	si sListeDestinataires ~= "" alors
      		sListeDestinataires = <§$0023§>
      	FIN
      	
      	// Expéditeur du message
      	si message.IDExpéditeur = -1 alors
      		sExpéditeur = RécupèreNomAffichéEmail(message.EmailExpéditeur)
      	sinon
      		sExpéditeur = RécupèreNomUtilisateur(message.IDExpéditeur)
      		sSignature  = HHCodeVersHTML(RécupèreSignatureUtilisateur(Message.IDExpéditeur, vrai))
      	FIN
      	
      	// Récupère les pièces jointes
      	pour tout MessageAttache avec IDMessage = gnIDMessage
      		
      		sListePiècesJointes += [rc] + MessageAttache.Clé
      		
      	FIN
      	
      	//
      	// Affichage
      	//
      	
      	// Expéditeur et destinataires
      	LIBHTM_EXPDEST = chaineconstruit(LIBHTM_EXPDEST, sExpéditeur, sListeDestinataires)
      	
      	// Sujet
      	SI Message.Sujet ~= "" ALORS Message.Sujet = <§$0006§>
      	LIB_SUJET	   = message.Sujet
      	mapage..titre  = "Honolulu - " + Message.Sujet
      	
      	// Date
      	dhDateHeureRéception est une dateheure
      	dhDateHeureRéception = message.DateHeureEnvoi
      	
      	si dhDateHeureRéception..partiedate = datedujour ALORS
      		// On affiche l'heure
      		LIB_DATE = heureverschaine(dhDateHeureRéception..partieheure, "HH:MM") + chaineconstruit(" (%1)", DateHeureNaturelle(dhDateHeureRéception))
      	sinon
      		LIB_DATE = DateHeureNaturelle(dhDateHeureRéception, vrai)
      	FIN
      	
      	// Message
      	sMessage est une chaine
      	sMessage = message.Contenu
      	
      	// Transforme le message si nécessaire et ajoute la signature
      	sMessage = MessageUniforme(sMessage) + sSignature
      	
      	// Ajout des pièces jointes dans le message
      	si sListePiècesJointes <> "" alors
      		
      		nNbPiècesJointes est un entier
      		sClé, sURL est une chaine
      		sInfos, sNomFichier, sMIME sont des chaines
      		moTaille est un monétaire
      		
      		// Pour chaque pièce jointe
      		pour toute chaine sClé de sListePiècesJointes separee par rc
      			
      			// On vérifie que la pièce jointe existe toujours
      			si PJCléExiste(sClé) alors
      				
      				// Récupère toutes les infos de la pièce jointe
      				sURL = PJRécupèreURL(sClé)
      				PJRécupère(sClé, sInfos, sMIME, moTaille, sNomFichier)
      				
      				// Si c'est une image
      				si position(sMIME, "image", 0, sanscasse) > 0 ALORS
      					
      					// On affichera un aperçu de l'image
      					sHTMPiècesJointes += ["<br/>" + rc] + "<table border=0 cellpadding=2 cellspacing=2><tr><td width=100 height=100 style=""border:1px solid #c0c0c0"">" + ...
      										ChaîneConstruit("<a href=""javascript:PHONOLULUNAV_DOWNLOAD('%1');""><img src=""" + sURL + "&mode=homo&sizel=100&sizeh=100"" width=100 height=100 border=0 /></a></td>", sClé) + ...
      										"<td width=10>&nbsp;</td>" +  ...
      										"<td class=Libelle>" + ChaîneConstruit("<strong>%1</strong>", sNomFichier) + ...
      										"<br/>" + RC +  TailleVersChaîne(moTaille) + ESP + ...
      										ChaîneConstruit(<§$000e§>, sClé) + "</td></tr></table>"
      					
      				sinon
      					
      					// On affiche simplement un lien vers le fichier
      					sHTMPiècesJointes += ["<br/>" + rc] + "<p><span class=Libelle>" + chaineconstruit(""+TAB+""+TAB+""+TAB+""+TAB+""+TAB+""+TAB+""+TAB+""+TAB+""+TAB+""+TAB+" <img src=""%2"" width=16 height=16 />", sNomFichier, "/" + repertoireweb() + ...
      										"/FR/PAGE_PJ." + AWP_EXT + "?mime=" + fExtraitChemin(sNomFichier, fFichier+fExtension) + "&sizel=16&sizeh=16&AWPSESSIONID=" + gsIdentifiant) + esp + ...
      										 chaineconstruit("<strong>%1</strong>", sNomFichier) + ...
      										 "<br/>" + rc +  TailleVersChaîne(moTaille) + esp + ...
      										 chaineconstruit(<§$0016§>, sClé) + "spanan></p>"	
      				FIN
      				
      				// Comptage des pièces jointes
      				nNbPiècesJointes ++
      				
      			FIN
      			
      		FIN
      		
      		sHTMPiècesJointes = ChaîneConstruit(""+TAB+""+TAB+"<hr/>"+RC+""+TAB+""+TAB+"<br/>"+RC+""+TAB+""+TAB+"<div class=Libelle>"+RC+""+TAB+""+TAB+""+TAB+"<strong>%1</strong>"+RC+""+TAB+""+TAB+""+TAB+"<br/><br/>"+RC+""+TAB+""+TAB+""+TAB+"%2"+RC+""+TAB+""+TAB+"</div>", ChaîneConstruit(<§$0019§>, nNbPiècesJointes, (nNbPiècesJointes > 1 ? "s" SINON "")), sHTMPiècesJointes)	
      	FIN
      	
      	LIBHTM_MESSAGE..Valeur = ChaîneConstruit(""+TAB+"<div style=""position: relative; width:100%"">"+RC+""+TAB+""+TAB+"<table align=left valign=top>"+RC+""+TAB+""+TAB+""+TAB+"<tr>"+RC+""+TAB+""+TAB+""+TAB+""+TAB+"<td>"+RC+""+TAB+""+TAB+""+TAB+""+TAB+""+TAB+"%1"+RC+""+TAB+""+TAB+""+TAB+""+TAB+"</td>"+RC+""+TAB+""+TAB+""+TAB+"</tr>"+RC+""+TAB+""+TAB+"</table>"+RC+""+TAB+""+TAB+"<br>"+RC+""+TAB+"</div>", sMessage + sHTMPiècesJointes)
      		
      	// On ne peut pas répondre au service de notifications
      	si message.IDExpéditeur = idsystem() alors
      		LIEN_REPONDRE_MSG..etat = grise
      		LIEN_REPONDREATOUS_MSG..etat = grise
      	sinon
      		LIEN_REPONDRE_MSG..etat = actif
      		LIEN_REPONDREATOUS_MSG..etat = actif
      	FIN
      	
      	si message.EstSupprimé alors
      		LIEN_SUPPRIMER_MSG = <§$001c§>
      		LIEN_SUPPRIMER_MSG..bulle = <§$001d§>
      	sinon
      		LIEN_SUPPRIMER_MSG = <§$001e§>
      		LIEN_SUPPRIMER_MSG..Bulle = <§$001f§>
      	FIN
      
      	
