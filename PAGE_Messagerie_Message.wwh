      -
        internal_properties : CAAAAAgAAACcx5C0f/XChNHAQVU/Xb0DTXPKTSQIkgD6gBagAo5+zsO8NhFDP+mWk/P5myWARMpoYkMfmK+o9/Aj/hIGN9N9gkYIxqbzsOvbs0E0rt56LjX75kYPM7mRWWOH+IMRm/7RInmymEIkGspu4augx4UIEl0v5bbl40Ns2OLnH5XFK4aM
        code : |1-
         
         HLitRecherchePremier(message, idmessage, gnIDMessage)
         
         sMessage est une chaine
         sListeDestinataires est une chaine
         sDestinataire est une chaine
         
         // Message
         sMessage = Message.Contenu
         // Transforme le message si nécessaire
         sMessage = MessageUniforme(sMessage)
         sMessage = <§$0021§> + RC + Remplace(Remplace(Remplace(sMessage, "<br>", "<br/>", SansCasse), "<br/>", "<br/>&gt;&nbsp;"), "<p>", "<p>&gt;&nbsp;") + "</p>"
         
         // Reconstruit la liste des destinataires
         // Récupère les destinataires
         POUR TOUT Destinataire AVEC IDMessage = gnIDMessage
         	
         	// On n'affiche pas les destinataires cachés
         	SI Destinataire.EstDestinataireCaché ALORS CONTINUER
         	
         	// On ne tient pas compte de l'utilisateur courant
         	SI Destinataire.IDUtilisateur = UtilisateurEnCours:IDUtilisateur ALORS CONTINUER
         	
         	// Récupère l'utilisateur
         	SI Destinataire.IDUtilisateur <> -1 ALORS
         		sDestinataire = RécupèreLogin(Destinataire.IDUtilisateur)
         	sinon
         		sDestinataire = ExtraitChaîne(Destinataire.EmailDestinataire,2)	
         	fin
         
         	// On n'ajoute pas deux fois le meme destinataire
         	si position(sListeDestinataires, sDestinataire, 0, motcomplet) < 1 alors	
         		// C'est un utilisateur connu, on affiche son nom
         		sListeDestinataires += [";"] + sDestinataire
         	FIN
         	
         FIN
         
         // Ajoute l'expéditeur
         si Message.IDExpéditeur <> -1 alors
         	sListeDestinataires += [";"] + RécupèreLogin(message.IDExpéditeur)
         sinon
         	sListeDestinataires += [";"] + ExtraitChaîne(message.EmailExpéditeur, 2)
         fin
         
         // Répond au message (on ne transfère pas les pièces jointes)
         PageAffiche(PAGE_Messagerie_NouveauMessage, "Re : " + message.Sujet, sMessage, sListeDestinataires, "")
