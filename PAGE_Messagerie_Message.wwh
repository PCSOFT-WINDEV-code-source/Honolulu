      -
        internal_properties : CAAAAAgAAAAbUbshpY0HFzos9eSOQ+XeCZmtxqZZrG58A2HCnw5radoA0NvwIG8c33rVAcgxg9E1QmhcsxB7/93kRXEgQTEJFUPsUicTM1WvUVuYWp/z55gkDTzi/Q==
        code : |1-
         
         // Vérifie si le message a été envoyé par l'utilisateur en cours
         HLitRecherchePremier(Message, IDMessage, gnIDMessage)
         SI HTrouve(Message) ALORS
         	
         	// Le message a été envoyé par l'utilisateur en cours
         	SI Message.IDExpéditeur = UtilisateurEnCours:IDUtilisateur ALORS
         		
         		// On marque ce message comme supprimé
         		SI PAGE_PortailMessagerie.gnListe = BoîteSupprimés ALORS
         			Message.EstSupprimé = Faux
         		SINON
         			SI PAGE_PortailMessagerie.gnListe = BoîteEnvoyés ALORS
         				// On ne supprime pas le message envoyé
         				Message.EstSupprimé = Vrai
         			FIN
         		FIN			
         		
         		HModifie(Message)
         	FIN
         	
         	// On recherche dans les destinataires le message courant
         	POUR TOUT Destinataire AVEC IDUtilisateurParIDMessage = [UtilisateurEnCours:IDUtilisateur, gnIDMessage]
         		
         		// On marque ce message comme supprimé
         		SI PAGE_PortailMessagerie.gnListe = BoîteSupprimés ALORS
         			Destinataire.EstSupprimé = Faux
         		SINON
         			SI PAGE_PortailMessagerie.gnListe <> BoîteEnvoyés ALORS
         				// On ne supprime pas le message envoyé
         				Destinataire.EstSupprimé = Vrai
         			FIN
         		FIN	
         		
         		HModifie(Destinataire)
         	FIN
         	
         	
         	SAI_FERME = 1
         	
         FIN
        type : 851984
      -
        type : 1310736
        enabled : false
